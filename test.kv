#:kivy 1.0.9

<TestBody>:
    size: 50, 50
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size

<TestGame>:
    body: test_body

    canvas:
        Rectangle:
            pos: self.center_x - 5, 0
            size: 10, self.height

    TestBody:
        id: test_body
        center: self.parent.center
